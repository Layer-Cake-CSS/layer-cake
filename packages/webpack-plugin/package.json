{
  "name": "@layer-cake/webpack-plugin",
  "version": "0.0.0",
  "main": "dist/layer-cake-webpack-plugin.cjs.js",
  "module": "dist/layer-cake-webpack-plugin.esm.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./dist/layer-cake-webpack-plugin.esm.js",
      "default": "./dist/layer-cake-webpack-plugin.cjs.js"
    },
    "./loader": {
      "module": "./loader/dist/layer-cake-webpack-plugin-loader.esm.js",
      "default": "./loader/dist/layer-cake-webpack-plugin-loader.cjs.js"
    },
    "./virtualFileLoader": {
      "module": "./virtualFileLoader/dist/layer-cake-webpack-plugin-virtualFileLoader.esm.js",
      "default": "./virtualFileLoader/dist/layer-cake-webpack-plugin-virtualFileLoader.cjs.js"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "/dist",
    "/loader",
    "/virtualFileLoader",
    "extracted.js"
  ],
  "peerDependencies": {
    "webpack": "^4.30.0 || ^5.20.2"
  },
  "dependencies": {
    "@layer-cake/integration": "*",
    "loader-utils": "^2.0.0"
  },
  "devDependencies": {
    "@types/loader-utils": "^2.0.4",
    "webpack": "^5.36.1"
  }
}
