{
  "extends": "@tsconfig/node16-strictest-esm/tsconfig.json",
  "include": ["."],
  "exclude": ["dist", "build", "node_modules"],
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "importsNotUsedAsValues": "remove",
    "noPropertyAccessFromIndexSignature": false
  }
}
