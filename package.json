{
  "name": "layer-cake",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*",
    "tools/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=!./examples/*",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "syncpack": "^12.3.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.19.2"
}
